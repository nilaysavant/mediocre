  "scripts": {
    "prebuild": "rimraf ./build",
    "devfe": "cross-env BROWSER=none craco start",
    "build:fe": "cross-env CI='' craco build",
    "test:fe": "craco test",
    "eject:cra-fe": "react-scripts eject",
    "build:tauri": "tauri build",
    "build:tauri-debug": "tauri build --debug",
    "build": "npm run build:fe && npm run build:tauri",
    "build:debug": "npm run build:fe && npm run build:tauri-debug",
    "start": "concurrently -p \"[{name}]\" -n \"front-end,tauri\" -c \"bgGreen.bold,bgCyan.bold\" \"npm run devfe\" \"npm run devbe\"",
    "devbe": "cross-env RUST_DEBUG=1 tauri dev",
    "gen:theme-typings": "chakra-cli tokens src/theme.ts",
    "postinstall": "npm run gen:theme-typings",
    "prettify": "prettier --write .",
    "prepare": "node ./scripts/prepare.js",
    "git-pre-push": "node ./scripts/git-pre-push.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
